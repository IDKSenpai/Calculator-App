<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <StartupObject>Calculator.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons8-calculator-256.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\Img\icons8-calculator-256.png" />
    <Content Include="icons8-calculator-256.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons8-calculator-256.ico" />
    <Resource Include="icons8-calculator-256.png" />
  </ItemGroup>

</Project>
